# Ethereum RPC URL for Goerli testnet
GOERLI_RPC_URL=https://goerli.infura.io/v3/YOUR_INFURA_KEY

# Private key (without 0x prefix) for the deployer/operator wallet
# WARNING: Never commit your actual private key to git!
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Address of the deployed FlashloanArb contract
# Fill this in after deployment
CONTRACT_ADDRESS=

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Gas price in gwei for transactions (optional)
GAS_PRICE=50

# Set to true for dry-run mode (will not execute transactions)
DRY_RUN=true
